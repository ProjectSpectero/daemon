{
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },
  "Console": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "Daemon": {
    "PasswordCostCalculationIterations": 50,
    "PasswordCostLowerThreshold": 10,
    "PasswordCostTimeThreshold": 100,
    "PasswordCostCalculationTestTarget": "srU/Lz4RYsz}U[D-e-5Tv+>'$TwT='dvV?58",
    "JWTTokenExpiryInMinutes": 60, 
    "LocalSubnetBanEnabled": true,
    "RespectEndpointToOutgoingMapping": true,
    "BindToUnbound":  true, 
    "AuthCacheMinutes": 5,
    "DatabaseFile": "Database/db.sqlite",
    "WebRoot": "wwwroot",
    "SpaMode": true,
    "SpaFileName": "index.html", 
    "TemplateDirectory": "Templates",
    "LoggingConfig":  "nlog.config", 
    "BlockedRedirectUri": "https://blocked.spectero.com/?reason={0}&uri={1}"
  }
}