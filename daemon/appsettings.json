{
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },
  "Console": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "Daemon": {
    "AutoStartServices":  true, 
    "PasswordCostCalculationIterations": 50,
    "PasswordCostLowerThreshold": 10,
    "PasswordCostTimeThreshold": 100,
    "PasswordCostCalculationTestTarget": "srU/Lz4RYsz}U[D-e-5Tv+>'$TwT='dvV?58",
    "JWTTokenExpiryInMinutes": 60,
    "LogCommonProxyEngineErrors":  true, 
    "LocalSubnetBanEnabled": false,
    "RespectEndpointToOutgoingMapping": true,
    "BindToUnbound": true,
    "AuthCacheMinutes": 5,
    "InMemoryAuth": true,
    "InMemoryAuthCacheMinutes": 1,
    "DatabaseFile": "Database/db.sqlite",
    "WebRoot": "wwwroot",
    "SpaMode": true,
    "SpaFileName": "index.html",
    "SpaCacheTime": 1,
    "TemplateDirectory": "Templates",
    "LoggingConfig": "nlog.config",
    "BlockedRedirectUri": "https://blocked.spectero.com/?reason={0}&uri={1}&data={2}",
    "DefaultOutgoingIPResolver": "https://api.ipify.org/"
  }
}